- semantic_form_for @organization, :url=>admin_organizations_path do |f|
  - f.inputs do
    = f.input :name
    = f.input :street1
    = f.input :street2
    = f.input :city
    = f.input :state
    = f.input :zip
    = f.input :contact_person
    = f.input :phone_number
    = f.input :overview

  = f.submit

  